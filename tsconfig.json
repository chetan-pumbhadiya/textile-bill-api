{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "outDir": "dist",
        "rootDir": "src",
        "strict": false,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*", "routes", "src/types", "utils", "controllers"],
    "exclude": ["src/types/*.ts", "node_modules", ".vscode"]
}
